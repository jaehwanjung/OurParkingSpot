<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>guestbook</servlet-name>
        <servlet-class>com.ourparkingspot.main.GuestBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>guestbook</servlet-name>
        <url-pattern>/guestbook</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>bookingQuery</servlet-name>
        <servlet-class>com.ourparkingspot.main.BookingQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bookingQuery</servlet-name>
        <url-pattern>/bookingQuery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>markerQuery</servlet-name>
        <servlet-class>com.ourparkingspot.main.MarkerQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>markerQuery</servlet-name>
        <url-pattern>/markerQuery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>hosting</servlet-name>
        <servlet-class>com.ourparkingspot.main.HostingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hosting</servlet-name>
        <url-pattern>/hosting</url-pattern>
    </servlet-mapping>
	<servlet>
		<servlet-name>booking</servlet-name>
		<servlet-class>com.ourparkingspot.main.BookingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>booking</servlet-name>
		<url-pattern>/booking</url-pattern>
	</servlet-mapping>
	<servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>com.ourparkingspot.main.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>main.jsp</welcome-file>
	</welcome-file-list>
</web-app>
